# AI Worker Environment Configuration
# Copy this file to .env and fill in your values

# ============ Discord Configuration ============
# Get your token from: https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_PREFIX=!

# ============ Feishu Configuration ============
# Get your credentials from: https://open.feishu.cn
FEISHU_APP_ID=your_feishu_app_id_here
FEISHU_APP_SECRET=your_feishu_app_secret_here
FEISHU_VERIFICATION_TOKEN=your_verification_token_here
FEISHU_ENCRYPT_KEY=your_encrypt_key_here

# ============ OpenAI Configuration ============
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=
OPENAI_MODEL=gpt-4o
OPENAI_MAX_TOKENS=4096
OPENAI_TEMPERATURE=0.7

# ============ Web Search Configuration ============
# Tavily API (optional, falls back to DuckDuckGo if not set)
# Get your API key from: https://tavily.com
TAVILY_API_KEY=

# ============ General ============
DEBUG=false
